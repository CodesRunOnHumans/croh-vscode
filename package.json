{
    "name": "croh-vscode",
    "displayName": "Croh - Codes Run On Humans",
    "description": "Croh support for Visual Studio Code",
    "version": "1.0.0",
    "icon": "./ico.png",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/CodesRunOnHumans/croh-vscode.git"
    },
    "contributes": {
        "languages": [{
            "id": "codes-run-on-humans",
            "aliases": ["Croh", "codes-run-on-humans"],
            "extensions": [".croh"],
            "configuration": "./language-configuration.json"
        }],
        "iconThemes": [{
            "id": "codes-run-on-humans",
            "label": "Croh",
            "path": "./fileicons/croh-icon-theme.json"
        }]
    }
}